{
  "version": 2,
  "name": "weread-clone",
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "DATABASE_URL": "${DATABASE_URL}",
    "NEXTAUTH_URL": "${NEXTAUTH_URL}",
    "NEXTAUTH_SECRET": "${NEXTAUTH_SECRET}"
  },
  "build": {
    "env": {
      "DATABASE_URL": "${DATABASE_URL}",
      "NEXTAUTH_URL": "${NEXTAUTH_URL}",
      "NEXTAUTH_SECRET": "${NEXTAUTH_SECRET}"
    }
  },
  "functions": {
    "app/api/**/*.ts": {
      "maxDuration": 30
    }
  }
}